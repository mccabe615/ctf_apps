<h1> User Management </h1>
<div id="user_nav" style="position: absolute; top: 0; right: 0; width: auto; text-align:right;">
  <% if current_user %>
    <!--
    #This is my XSS-able version
    Logged in as <%= raw(current_user.email) %>.
    -->
    Logged in as <%= current_user.email %>.
    <%= link_to "Log out", log_out_path %>
  <% end %>
</div>
<table align="center" border="1" cellpadding="5" cellspacing="5" width="75%">
  <tr>
  <th>Username</th>
  <th>Role: Admin</th>
  </tr>
  <% users.each do |user| %>   
  <tr>  
    <td width="20%"><%= user.email%></td><td><%=user.admin?%></td>
  </tr>
  <% end %>
  </table>


